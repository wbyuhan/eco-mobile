{
  "name": "eco-mobile",
  "version": "1.1.2",
  "description": "基于react的移动端扩展组件",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "complete": "npm run docs:build && npm run release"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "author": "Ecode",
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yicoding/eco-mobile.git"
  },
  "keywords": [
    "react",
    "react-components",
    "antd",
    "antd-mobile",
    "ant-design-mobile",
    "eco-mobile"
  ],
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@wonder-ui/core": "^1.3.10",
    "@wonder-ui/styles": "^2.0.0-beta.9",
    "react": ">= 16.9.0",
    "react-dom": ">= 16.9.0"
  },
  "dependencies": {
    "@wonder-ui/core": "^1.3.10",
    "@wonder-ui/styles": "^2.0.0-beta.9",
    "antd-mobile": "^2.3.4",
    "classnames": "^2.2.6",
    "compressorjs": "^1.0.7",
    "moment": "^2.29.1",
    "rc-field-form": "^1.17.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-wx-images-viewer": "^1.0.6"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "compression-webpack-plugin": "^6.1.1",
    "dumi": "^1.0.38",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
